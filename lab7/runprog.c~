#include <stdio.h>
#include<string.h>
#include <unistd.h>
#include <sys/wait.h>
#include <sys/types.h>

int main (int argc, char ** argv)
{
  int j;
  char* command=argv[1];
  printf("%s ", command );
  
  for (j=2;j<argc;j++) {
    printf("%s ", argv[j] );
  }
  printf("\n");

  
  int status=-1;
  int pid = fork();

  if(pid)  //fork != 0
  {
    wait(&status); 
    printf("exit status: %d , %d ", status, WEXITSTATUS);
  }
    else { 

      execvp(command,argv+1);
            printf("kinder is alive error!\n");
  }

  return 0;
}